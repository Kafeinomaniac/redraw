{
  "extends": "airbnb",
  "env": {
    "jasmine": true,
    "browser": true
  },
  "globals": {
    "fabric": true
  },
  "rules": {
    "no-use-before-define": ["error", { "functions": false, "variables": true, "classes": true }],
    "comma-dangle": ["error", "never"],
    "no-underscore-dangle": ["error", { "allow": ["_this"] }],
    "new-cap": ["error", { "properties": false }],
    "no-new": "off"
  }
}
